<?xml version="1.0" encoding="utf-8"?>
<ripple xmlns:android="http://schemas.android.com/apk/res/android" android:color="@color/brandSecondary400">
    <item android:id="@android:id/mask">
        <shape android:shape="rectangle">
            <solid android:color="@color/brandSecondary400" />
            <corners android:radius="@dimen/cornerRadiusSmall" />

        </shape>
    </item>

    <item android:id="@android:id/background">
        <shape android:shape="rectangle">
            <corners android:radius="@dimen/cornerRadiusSmall" />
            <stroke android:width="3px" android:color="@color/brandPrimary500" />
        </shape>
    </item>
</ripple>