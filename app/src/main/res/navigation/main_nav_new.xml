        <argument
            android:name="category"
            app:argType="string" />
        <action
            android:id="@+id/action_discoverCategoryFragment_to_discoverFeaturedCastsFragment"
            app:destination="@id/discoverFeaturedCastsFragment" />
    </fragment>
